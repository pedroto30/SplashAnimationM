<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo02"
    tools:context=".ui.gallery.GalleryFragment">
    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <ImageView
        android:id="@+id/imagen02"
        android:layout_width="match_parent"
        android:layout_height="256dp"
        app:srcCompat="@drawable/cusco02" />
        <ImageView
            android:id="@+id/imagen03"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco03" />
        <ImageView
            android:id="@+id/imagen04"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco04" />
        <ImageView
            android:id="@+id/imagen05"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco05" />
        <ImageView
            android:id="@+id/imagen06"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco06" />
        <ImageView
            android:id="@+id/imagen07"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco07" />
        <ImageView
            android:id="@+id/imagen08"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            app:srcCompat="@drawable/cusco08" />
    </LinearLayout>
    </ScrollView>
    <TextView
        android:id="@+id/text_gallery"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="0sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
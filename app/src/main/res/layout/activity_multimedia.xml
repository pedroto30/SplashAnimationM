<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Multimedia">

    <ImageButton
        android:id="@+id/btnGato"
        android:layout_width="183dp"
        android:layout_height="150dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gato" />

    <ImageButton
        android:id="@+id/btnPerro"
        android:layout_width="163dp"
        android:layout_height="155dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnGato"
        app:srcCompat="@drawable/perro" />

    <ImageButton
        android:id="@+id/btnPayaso"
        android:layout_width="118dp"
        android:layout_height="110dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="316dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/payaso" />

    <ImageView
        android:id="@+id/imgFondo"
        android:layout_width="266dp"
        android:layout_height="231dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPayaso" />


</androidx.constraintlayout.widget.ConstraintLayout>